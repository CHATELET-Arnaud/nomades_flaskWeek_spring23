{% extends 'includes/base.html'%}
{% block title%} Account {% endblock %}
{% block content %}
    <p class="h1">Binevenue sur la page account</p>
    <a href="{{ url_for('index', nom='monica', age=23) }}">return to main page</a>
    <br/>

    <div>
        <ul>
            {% for eleve in eleves %}
              {% if eleve == "Monica"%} 
                <li>{{ eleve | upper }}</li>
              {% else %}
                <li>{{ eleve }}</li>
              {% endif %}
            {% endfor %}
        </ul>
    </div>
{% endblock %}